public class BankingGenericsDemo {
	
	public static void main(String[] args) {
		
		//Using Generic Class for different types of account numbers
		BankAccount<Integer> savAccount = new BankAccount<Integer>("Allice", 123456);
		BankAccount<String> currAccount = new BankAccount<String>("Bob", "CURR-3423");
		BankAccount<Long> loanAccount = new BankAccount<Long>("cHARLIE", 9867654321L);
		
	}

}
