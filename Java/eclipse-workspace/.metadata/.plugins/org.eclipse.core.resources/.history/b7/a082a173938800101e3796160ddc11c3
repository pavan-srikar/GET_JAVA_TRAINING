public class Complex {
	//Program to add 2 complex numbers- Real & Imaginary part


	private double real;
	private double imaginary;

	// Generate Constructor using fields




	public void display()
	{
		System.out.println("r"+this.real+" + i"+this.imaginary);
	}
}
