class Faculty{
	int empId;
	String name;

	public void setData(int c,String d){
		this.empId=c;
		this.name=d;
	}

	public void showData(){
		System.out.print("EmpId = "+empId + "  " + " Employee Name = "+name);
		System.out.println();
	}
}
