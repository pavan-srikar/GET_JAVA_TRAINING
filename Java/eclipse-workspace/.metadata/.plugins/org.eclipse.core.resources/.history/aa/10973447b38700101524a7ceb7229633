public class EnhancedForLoopDemo {

	public static void main(String[] args) {
		String array[] = {"Ron", "Ravi", "DiHh", "sigma", "fgsfg", "pavan" };
		
		//enhanced for loop
		for(String x:array) {
			System.out.println(x);
		}

	}

}
